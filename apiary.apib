{
  "url": "/managers/UUID",
  "username": "john", 
  "firstName": "John",
  "lastName": "Smith",
  "dateCreated": "2012-07-01T14:22:20.029Z", // ISO 8601 Date string
  "dateUpdated": "2012-07-01T14:22:20.029Z" // ISO 8601 Date string
